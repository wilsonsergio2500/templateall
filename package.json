    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "path-complete-extname": "^1.0.0",
    "string-template": "^1.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "underscore.string": "^3.3.5"
  }
}
