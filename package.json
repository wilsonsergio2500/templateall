{
  "name": "templateall",
  "version": "1.0.0",
  "description": "just a typescript example",
  "main": "./src/index.ts",
  "scripts": {
    "test": "test",
    "start": "node out/index.js",
    "prestart": "npm run build",
    "build": "tsc",
    "deploy": "node ./node_modules/typescript/bin/tsc ./src/index.ts --outDir ./dist",
    "compile": "babel src --out-dir .compiled --source-maps --watch",
    "templateall-generate": "node ./bin/initial.js"
  },
  "bin": {
    "templateall-generate": "node ./bin/initial.js"
  },
  "keywords": [
    "templateall"
  ],
  "author": "sergio wilson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsonsergio2500/templator.git"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.44",
    "@types/js-beautify": "^1.8.1",
    "@types/lodash": "^4.14.136",
    "@types/mkdirp": "^0.5.2",
    "@types/mkdirp-promise": "^5.0.0",
    "@types/node": "^8.0.14",
    "@types/string-template": "^1.0.2",
    "@types/underscore.string": "0.0.38",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "typescript": "^3.5.3"
  },
  "babel":{
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "errorlog": "^1.4.6",
    "inquirer": "^6.5.1",
